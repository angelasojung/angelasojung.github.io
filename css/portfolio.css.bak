@charset "utf-8";
@import url(http://fonts.googleapis.com/css?family=Righteous);


@font-face {
        font-family: 'Google-spies'; /* 글꼴 */
        src: local('Google-spies'),
            url('../font/Google-spies.eot'),
            url('../font/Google-spies.woff') format('woff'),
            url('../font/Google-spies.ttf') format('truetype');
			}


/*  Reset CSS  */
* { margin:0;  padding:0;}
ol,ul { list-style: none; }
a { outline:0; text-decoration:none; color:#fff; }
img { border:0;}
body { font-size:12px/1.6; color:#fff; } 


/*로고*/
h1 { position:fixed; top:0px; left:20px; z-index:10;font-family:righteous; font-size:40px; color:white;}
/*h1 img {width:200px;}*/

