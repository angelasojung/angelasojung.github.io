/*메뉴*/
#menu { position:fixed; top:50%; left:0px; margin-top:-100px; z-index:100;}
#menu a { display:block; width:130px; height:40px; font-family:righteous; 
font-size:17px; color:#fff; text-align:right; overflow:hidden; box-sizing:border-box;  
border-top:2.5px solid #fff;  /*opacity:0.8; */transition:all 0.5s;}
#menu li:hover a { font-size:20px;  opacity:1; }
#menu li.on a { font-size:20px; opacity:1;}

#menu li:nth-child(1):hover a { color:#ffa8d7; border-color:#ffa8d7;}
#menu li:nth-child(2):hover a { color:#dc4444; border-color:#dc4444;}
#menu li:nth-child(3):hover a { color:#ffec6a; border-color:#ffec6a;}
#menu li:nth-child(4):hover a { color:#5ac6ff; border-color:#5ac6ff;}
#menu li:nth-child(5):hover a { color:#5aff9c; border-color:#5aff9c;}
#menu li:nth-child(6):hover a { color:#dc73ff; border-color:#dc73ff;}


#menu li:nth-child(1).on a { color:#ffa8d7; border-color:#ffa8d7;}
#menu li:nth-child(2).on a { color:#dc4444; border-color:#dc4444;}
#menu li:nth-child(3).on a { color:#ffec6a; border-color:#ffec6a;}
#menu li:nth-child(4).on a { color:#5ac6ff; border-color:#5ac6ff;}
#menu li:nth-child(5).on a { color:#5aff9c; border-color:#5aff9c;}
#menu li:nth-child(6).on a { color:#dc73ff; border-color:#dc73ff;}


/*레이아웃*/
section { width:100%; height:700px; position:relative; background-size:cover; 
background-position:right center; background-attachment:fixed; overflow:hidden;}
section:nth-child(1) { background:url(../images/IMG_353381-2.jpg) no-repeat;background-size:cover;; }
section:nth-child(2) { background:url(../images/wood_texture3850-2.jpg) no-repeat;background-size:cover; }
section:nth-child(3) { background:url(../images/wildtextures-dark-wood-board.jpg) no-repeat;background-size:cover;}
section:nth-child(4) { background:url(../images/wildtextures-old-wood-original-file-1.jpg) no-repeat;background-size:cover; }
section:nth-child(5) { background:url(../images/texture-of-burnt-wood.jpg) no-repeat;background-size:cover; }
section:nth-child(6) {  background:url(../images/IMG_353399-1.jpg) no-repeat;background-size:cover;  }

/*텍스트 박스*/
article { width:80%;position:absolute; top:20%; left:20%; z-index:11;color:white;}
.artboard {width:65%;position:absolute; top:20%; left:20%; z-index:11;color:white;}

.design{font:70px Google-spies;color:#fff;margin-bottom:20px;}
article span{ font-size:20px; color:black;position:absolute; bottom:0%;right:10%;}
section:nth-child(1) p { color:#fff;}
section:nth-child(1) span { color:#111;}

section:nth-child(2) p { color:#fff; }
section:nth-child(2) span { color:#fff;}


section:nth-child(3) p { color:#fff;}
